#include<iostream>
using namespace std;
int main()
{
	int n,so=0,se=0;
  cin>>n;
  if(n<=15)
  {
  int a[n+1];
    for(int i=0;i<n;i++)
    cin>>a[i];
    for(int i=0;i<n;i++)
    {
    if(a[i]%2 == 0 )
      se = se + a[i];
      else
        so = so+a[i];
    }
    cout<<"The sum of the even numbers in the array is "<<se<<"\n";
    cout<<"The sum of the odd numbers in the array is "<<so;
  }
}
