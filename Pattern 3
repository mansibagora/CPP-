#include<iostream>
using namespace std;
int main()
{
  int n,t;
  cin>>n;
  for(int i=1;i<=n;i++)
  {
  for(int j=1;j<=i;)
  {
  cout<<i;
    j++;
    if(j>i)
      break;
    std::cout<<"*";
  }
    cout<<"\n";
  }
  for(int i=n;i>=1;i--)
  {
  for(int j=1;j<=i;)
  {
  cout<<i;
    j++;
    if(j>i)
      break;
    std::cout<<"*";
  }
    cout<<"\n";
  }
  return 0;
}
