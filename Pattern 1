#include<iostream>
using namespace std;
int main()
{
int n;
  std::cin>>n;
  for(int i=0;i<=3;i++)
  {
  for(int j=0;j<=i;j++)
  {
  std::cout<<n;
  }
    n++;
    std::cout<<"\n";
  }
  n--;
  for(int i=3;i>=0;i--)
  {
  for(int j=0;j<=i;j++)
  {
  std::cout<<n;
  }
    n--;
    std::cout<<"\n";
  }
return 0;
}
